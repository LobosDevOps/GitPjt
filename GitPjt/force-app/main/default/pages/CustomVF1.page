<apex:page standardController="CustumObj1__c" >
    <apex:form >
        <apex:pageBlock title="Edit Contact">
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!CustumObj1__c.Name}"/>
                <apex:inputField value="{!CustumObj1__c.CustomDate1__c}"/>
                <apex:inputField value="{!CustumObj1__c.CustomPicklst1__c}"/>
                <apex:inputField value="{!CustumObj1__c.CustomText1__c}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>